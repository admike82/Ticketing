{% extends 'base.html.twig' %}

{% block body %}

	{# navbar #}
	{{ component('NavbarComponent') }}


	<main class="h-full">
		{% for message in app.flashes('success') %}
			<div class="alert-success">
				{{ message | nl2br }}
			</div>
		{% endfor %}
		{% for message in app.flashes('danger') %}
			<div class="alert-danger">
				{{ message | nl2br }}
			</div>
		{% endfor %}
		{% block content %}{% endblock %}
	</main>

{% endblock %}
